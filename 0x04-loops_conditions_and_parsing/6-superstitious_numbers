#!/usr/bin/env bash
# Script that displays current directory contents with only the part after the first dash

for file in *; do
    # Check if the file exists and is not hidden
    if [[ -e "$file" && "$file" != .* ]]; then
        # Extract the part after the first dash
        if [[ "$file" == *-* ]]; then
            echo "${file#*-}"
        else
            echo "$file"
        fi
    fi
done
