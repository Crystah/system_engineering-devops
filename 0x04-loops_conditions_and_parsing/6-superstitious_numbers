#!/usr/bin/env bash
# Script that displays current directory contents with only the part after the first dash

for file in *; do
    # Skip if file doesn't exist or is hidden
    if [[ ! -e "$file" ]] || [[ "$file" == .* ]]; then
        continue
    fi
    
    # Extract the part after the first dash, or show whole name if no dash
    if [[ "$file" == *-* ]]; then
        echo "${file#*-}"
    else
        echo "$file"
    fi
done
